<table class="table table-striped table-bordered table-hover" id="friends">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Twitter</th> 
      <th>User Id</th> 
    </tr>
  </thead>

  <tbody>
    <% @friends.each do |friend| %>
    <tr>
      <%= render friend %>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Friend", new_friend_path, class:"btn btn-secondary" %>
